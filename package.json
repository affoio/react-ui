{
  "name": "@affoio/react-ui",
  "description": "An enterprise-class UI design language and React-based implementation",
  "main": "dist/index.js",
  "version": "1.1.50",
  "repository": {
    "type": "git",
    "url": "https://github.com/affoio/react-ui"
  },
  "license": "ISC",
  "scripts": {
    "start": "NODE_ENV=development babel-node dev/server",
    "start:prod": "NODE_ENV=production babel-node dev/server",
    "start:production": "PUBLIC_URL='/' yarn build && yarn start:prod",
    "prebuild": "yarn build:clean",
    "build:clean": "rimraf ./dist",
    "build": "NODE_ENV=production webpack --config config/internals/webpack/webpack.base.babel.js",
    "test": "NODE_ENV=test jest",
    "test:w": "NODE_ENV=test jest --watch",
    "storybook": "start-storybook -p 5000",
    "lint": "eslint \"./src/**/*.{js,jsx}\""
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged"
    }
  },
  "peerDependencies": {
    "react": "16.9.0",
